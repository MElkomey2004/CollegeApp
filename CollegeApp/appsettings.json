{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    },
    "Console": { // Console provider.
      "LogLevel": {
        "Default": "Error", // Overrides preceding LogLevel:Default setting.
        "Microsoft.Hosting" : "Trace"
      }
    },
    "Debug": { // Debug provider.
      "LogLevel": {
        "Default": "Trace", // Overrides preceding LogLevel:Default setting.
        "Microsoft.Hosting" : "Error"
      }
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "CollegeAppDBConnection": "Data Source=(localdb)\\MSSQLLocalDB;Initial Catalog=CollegeAppDB;Integrated Security=True;"
  }
}
